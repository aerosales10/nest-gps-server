{
  "name": "@aerosales10/nest-gps-server",
  "private": false,
  "version": "1.0.0",
  "description": "A GPS server for Nestjs",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "discovery",
    "modules",
    "GPS"
  ],
  "author": {
    "name": "Alberto E. Rosales"
  },
  "main": "dist/libs/gps-server/index.js",
  "typings": "dist/libs/gps-server/index.d.ts",
  "scripts": {
    "build": "nest build gps-server"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aerosales10/nest-gps-server"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.7"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.7",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.7",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/gps-server"
    ],
    "moduleNameMapper": {
      "@miraibit/gps-server/(.*)": "<rootDir>/libs/gps-server/src/$1",
      "@miraibit/gps-server": "<rootDir>/libs/gps-server/src"
    }
  }
}
